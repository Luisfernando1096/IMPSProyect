<div class="container p-4">
    <div class="col-sm-12">
        <div class="row">
            <div class="col-sm-6">
                <h3>Listado profesores</h3>
            </div>
            <div class="col-sm-6">
                <a href="/profesores/agregar" class="btn btn-success d-block
text-white">Agregar profesor</a>
            </div>
        </div>
    </div>
    <div class="col-sm-12 mt-4">
        <table class="table table-bordered table-striped">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">Codigo</th>
                    <th scope="col">profesor</th>
                    
                </tr>
            </thead>
            <tbody>
                {{#each profesores}}
                <tr>
                    <th scope="row">{{idprofesor}}</th>
                    <td>{{profesor}}</td>
                    <td>
                        <a class="btn btn-primary text-white" href="/profesores/editar/{{idprofesor}}">Modificar</a>
                        <a href="/profesores/eliminar/{{idprofesor}}" class="btn
btn-danger text-white">Eliminar</a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>